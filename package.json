{
  "name": "dorkroomreact",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "bun ./scripts/reset-project.js",
    "android": "DARK_MODE=media expo start --android",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "bun test",
    "lint": "expo lint",
    "dev": "bun turbo run dev",
    "build": "bun turbo run build",
    "clean": "bun turbo run clean && rm -rf node_modules",
    "vercel-build": "NODE_OPTIONS='--max-old-space-size=8192' EXPO_ROUTER_STATIC_RENDERING=1 npx expo export -p web",
    "build-minimal": "NODE_OPTIONS='--max-old-space-size=8192' EXPO_PUBLIC_DISABLE_STATIC_RENDERING=1 npx expo export -p web",
    "build-diagnostics": "NODE_OPTIONS='--max-old-space-size=4096 --trace-warnings' DEBUG=expo:* bunx expo export -p web",
    "deploy": "bunx vercel deploy"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
