{
  "name": "dorkroomreact",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "bun ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "bun test",
    "lint": "expo lint",
    "dev": "expo start --go",
    "build": "expo export -p web",
    "clean": "rm -rf node_modules .expo dist",
    "web-export": "bunx expo export -p web",
    "deploy": "bunx vercel deploy",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        2,
        "always",
        120
      ]
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/html-elements": "^0.4.3",
    "@expo/vector-icons": "^14.1.0",
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/actionsheet": "^0.2.53",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/icon": "^0.1.27",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.22",
    "@gluestack-ui/select": "^0.1.31",
    "@gluestack-ui/spinner": "^0.1.15",
    "@gluestack-ui/themed": "^1.1.73",
    "@gluestack-ui/toast": "^1.0.9",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli": "^14.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native-picker/picker": "2.11.0",
    "@react-native/normalize-colors": "^0.79.5",
    "@vercel/speed-insights": "^1.2.0",
    "expo": "53.0.12",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.3",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linking": "^7.1.6",
    "expo-router": "~5.1.2",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "fuse.js": "^7.1.0",
    "lucide-react-native": "^0.514.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.14",
    "@types/react-test-renderer": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/node": "^5.3.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.8",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3",
    "vercel": "^32.7.2"
  },
  "private": true
}
