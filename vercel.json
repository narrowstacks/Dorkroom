{
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "framework": "static",
  "buildCommand": "bun run vercel-build",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install"
}
