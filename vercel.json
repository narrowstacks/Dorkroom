{
  "buildCommand": "bunx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "bun run dev",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install"
}
